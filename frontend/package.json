  "private": true,
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ../app/src/main/resources/static/dist ../app/src/main/resources/templates/index.html ci",
    "lint": "eslint src test --fix",
    "lint:checkstyle": "mkdirp ci && eslint src --format checkstyle > ci/checkstyle.xml; exit 0",
    "test": "mocha",
    "coverage": "cross-env NODE_ENV=test nyc mocha --reporter mocha-junit-reporter --reporter-options mochaFile=./ci/test-results.xml",
    "dev": "cross-env PROFILE=local node index.js",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production webpack --verbose --colors --display-error-details --config webpack/build.js",
    "prestart": "pm2 ping",
    "start": "pm2 start app.json --no-daemon",
    "stop": "pm2 stop app.json && pm2 delete app.json"
  },
